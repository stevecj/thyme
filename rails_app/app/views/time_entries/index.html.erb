<% @date_cols.each do |col| %>
  <span id='<%=col[:id]%>' class='date-column'>
    <%=col[:date]%>
    <%= link_to 'Add', new_time_entry_path(:on_date=>col[:date]), :class=>'add-new' %>
    <span class='entry'>
      <span class='from-time' >10:30 AM</span>
      <span class='to-time'   >12:15 PM</span>
      <span class='activity'  >Weed whacking</span>
    </span>
  </span>
<% end %>
